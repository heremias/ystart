<?php

/**
 * @file
 * Install, update and uninstall functions for the config_notify module.
 *
 * Certain hooks will only work if they are placed in the .install file.
 */

/**
 * Implements hook_uninstall().
 */
function config_notify_uninstall() {
  \Drupal::state()->delete('config_notify.last_sent');
}
